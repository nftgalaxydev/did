{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "base_name",
    "base_node",
    "name",
    "registry_address",
    "symbol"
  ],
  "properties": {
    "base_name": {
      "type": "string"
    },
    "base_node": {
      "type": "string"
    },
    "grace_period": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "name": {
      "description": "Name of the NFT contract",
      "type": "string"
    },
    "registry_address": {
      "type": "string"
    },
    "symbol": {
      "description": "Symbol of the NFT contract",
      "type": "string"
    }
  }
}
